version "2021.0"

#EventWeather Field
#true and false needs to be lower case
EventWeather(input) = 
    case input == "HF" : false
    case input == "CU" : false
    case input == "BP" : false
    case input == "SBF" : false
    case input == "SF" : false
    case input == "R" : false
    else: true

# example of calling function
# EventWeather(inputName(EventName))
   